        <!-- Marker tracker -->
        <object
		class   = "Lines"
		module  = "MarkerTracker"
		source  = "EDGES"
		x = "0" y = "0"
		w = "6" h = "5"
		min = "0"
		max = "1"
                color="red"
                stroke_width="1"
	/>

	<object
		class   = "Cross"
		module  = "MarkerTracker"
		source  = "MARKERS"
                size = "0.03"
                stroke_width="1"
